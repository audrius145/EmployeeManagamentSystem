<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="700.0" spacing="70.0" userData="Edit Department" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.EditDepartmentController">
   <children>
      <HBox alignment="CENTER" prefHeight="99.0" prefWidth="500.0" spacing="10.0">
         <children>
            <Label text="Edit Department:">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label fx:id="id" text="10">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label text="(ID)">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin></HBox>
      <TextField fx:id="name" prefHeight="26.0" prefWidth="434.0" promptText="Name" />
      <HBox alignment="CENTER" prefHeight="82.0" prefWidth="400.0" spacing="50.0">
         <children>
            <Button mnemonicParsing="false" onAction="#saveEdit" prefWidth="59.0" text="Save" />
            <Button mnemonicParsing="false" onAction="#cancelEdit" prefHeight="25.0" prefWidth="59.0" text="Cancel" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets left="100.0" right="100.0" />
   </padding>
</VBox>
